<div class="item">
  <ul>
    <img class='style_image' src="<%= @item.pic_url %>">
    <br><h2><%= @item.name %></h2>
    <h2>Price $<%= @item.price %></h2>
  <ul>
</div>
<div class="buttons">
  <ul>
    <button class="button1"><span><%= link_to 'Add to Cart', add_to_cart_item_path(@item), method: :post%></span></button>
    <button class="button2"><span><%= link_to 'Edit', edit_item_path(@item)%></span></button>
  <ul>
</div>
